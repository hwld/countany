{
  "name": "countany",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "migrate-dev:save": "dotenv -e prisma/.env.dev -- npx prisma migrate save --experimental",
    "migrate-dev:up": "dotenv -e prisma/.env.dev -- npx prisma migrate up --experimental",
    "generate-dev": "cp prisma/.env.dev prisma/.env && prisma generate",

    "migrate-prod:save": "dotenv -e prisma/.env.prod -- npx prisma migrate save --experimental",
    "migrate-prod:up": "dotenv -e prisma/.env.prod -- npx prisma migrate up --experimental",
    "generate-prod": "cp prisma/.env.prod prisma/.env && prisma generate",
    
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@prisma/client": "^2.11.0",
    "framer-motion": "^2.9.4",
    "next": "9.5.5",
    "next-auth": "^3.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.10.0",
    "styled-components": "^5.2.0",
    "swr": "^0.3.8"
  },
  "devDependencies": {
    "@prisma/cli": "2.10.2",
    "@types/material-ui": "^0.21.8",
    "@types/next-auth": "^3.1.15",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-plugin-styled-components": "^1.11.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
